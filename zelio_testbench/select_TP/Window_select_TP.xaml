<Window x:Class="zelio_testbench.select_TP.Window_select_TP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:zelio_testbench.select_TP"
        mc:Ignorable="d"
        Title="Selectionner le TP" Height="450" Width="800" FontSize="30" FontWeight="Bold">
    <Grid Background="#FFE5E5E5">
        <ListBox x:Name="List_choix" HorizontalAlignment="Center" VerticalAlignment="Top" Width="780" Margin="0,30,0,0" RenderTransformOrigin="0.355,0.539" Height="236">
            <ListBoxItem Content="Operateur Mathematique" IsSelected="True"></ListBoxItem>
            <ListBoxItem Content="Chateau d'eau" ></ListBoxItem>
            <ListBoxItem Content="Feu Boite" ></ListBoxItem>
            <ListBoxItem Content="Ascenceur Boite" ></ListBoxItem>
            <ListBoxItem Content="Debug Only" ></ListBoxItem>
        </ListBox>
        <Button Content="Select" HorizontalAlignment="Center" Width="150" Margin="0,0,0,20" VerticalAlignment="Bottom" RenderTransformOrigin="0.355,0.539"  Click="Select_clicked"/>


    </Grid>
</Window>
